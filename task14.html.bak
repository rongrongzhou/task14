<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>IFE JavaScript Task 01</title>
  </head>
<body>

  <h3>污染城市列表</h3>
  <input type="button" value="空气质量指数高于60" onclick="test()"/>
  <ul id="aqi-list">
<!--   
    <li>第一名：福州（样例），10</li>
  	<li>第二名：福州（样例），10</li> -->
  </ul>

<script type="text/javascript">

var aqiData = [
  ["北京", 90],
  ["上海", 50],
  ["福州", 10],
  ["广州", 50],
  ["成都", 90],
  ["西安", 100]
];

function test() {
    
  /*
  在注释下方编写代码
  遍历读取aqiData中各个城市的数据
  将空气质量指数大于60的城市显示到aqi-list的列表中
  */
  //对空气质量指数进行排序
  var oList = document.getElementById("aqi-list");
  var arr=["1","2","3","4","5","6"];
 function compare(a,b){
  if (a[1]<b[1])
  {
	  return 1;
  }
  else if (a[1]>b[1])
  {
	  return -1;
  }
  else
  {
	  return 0;
  }
  }
  aqiData.sort(compare);
 //遍历二维数组 提出大于60的城市
  for (var i=0;i<aqiData.length ;i++ )
  {
	  
		   if (aqiData[i][1]>60)
	  {   
		  //alert(aqiData[i]+","+aqiData[i][j])
		  var oli=document.createElement("li");
		  oli.innerHTML="第"+arr[i]+"名："+aqiData[i][0]+","+aqiData[i][1];
		  alert("第"+arr[i]+"名："+aqiData[i][0]+","+aqiData[i][1]);
         oList.appendChild(oli);
	  }
	  

  }

}

</script>
</body>
</html>
